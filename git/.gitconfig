[core]
	ui = auto
	quotepath = false
	editor = vim
	excludesfile = ~/.gitignore_global
[init]
	defaultBranch = main
[push]
	autoSetupRemote = true
	default = current
[color]
	ui = auto

[include]
	path = ~/dotfiles/git/.gitconfig-yutatech.conf

